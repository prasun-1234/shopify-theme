<link rel="stylesheet" href="{{ 'main-page.css' | asset_url }}">

<section class="default-page">
  <div class="container">
    <div class="pure-g">
      <div class="pure-u-1 l-box">
        <h4>{{ page.title }}</h4>
        {{ page.content }}
      </div>
    </div>
  </div>
</section>

{% schema %}
{
  "name": "t:sections.page.name",
  "tag": "section",
  "class": "spaced-section"
}
{% endschema %}
