{% style %}
    .gps_ring {
      border: 2px solid #999;
      border-radius: 30px;
      width: 40px;
      height: 40px;
      -webkit-animation: pulsate 1s ease-out;
      -webkit-animation-iteration-count: infinite;
      opacity: 0;
    }
    .gps_ring2 {
      position: absolute;
      border: 2px solid #999;
      border-radius: 30px;
      width: 8px;
      height: 8px;
      left: 4px;
      top: 4px;
      background: red;
      -webkit-animation-iteration-count: infinite;
      opacity: 1;
    }
    .gps_ring1 {
      top: 8px;
      left: 8px;
      position: relative;
      border: 2px solid #999;
      border-radius: 30px;
      width: 20px;
      height: 20px;
     -webkit-animation-delay: 12s;
      animation-delay: 2s;
      -webkit-animation: pulsate 1s ease-out;
      -webkit-animation-iteration-count: infinite;
      opacity: 0;
    }
    
    @-webkit-keyframes pulsate {
      0% {
        -webkit-transform: scale(0.1,0.1);
        opacity: 0;
      }
      50% {
        opacity: 1;
      }
      100% {
        -webkit-transform: scale(1.2,1.2);
        opacity: 0;
      }
    }
  
    {% endstyle %}
    <div class="">
      <div class="gps_ring"><div class="gps_ring1"><div class="gps_ring2"></div></div></div>
      <div class=" absolute top-0 left-0 w-full h-full pointer-events-none">
        <div class=" w-full h-full flex justify-center items-center">
          <svg class="w-[40px] h-[40px]" width="25" height="25" viewBox="0 0 70 70" fill="none" xmlns="http://www.w3.org/2000/svg">
