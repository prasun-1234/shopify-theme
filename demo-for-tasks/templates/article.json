{
  "sections": {
    "breadcrumb": {
      "type": "breadcrumb",
      "settings": {
        "section_margin_top": 40,
        "section_margin_bottom": 40,
        "text_color": "#CBD5E1"
      }
    },
    "main": {
      "type": "main-article",
      "blocks": {
        "featured_image": {
          "type": "featured_image",
          "settings": { "image_height": "adapt" }
        },
        "title": {
          "type": "title",
          "settings": { "blog_show_date": true, "blog_show_author": false }
        },
        "share": { "type": "share", "settings": { "share_label": "Share" } },
        "content": { "type": "content", "settings": {} }
      },
      "block_order": ["featured_image", "title", "share", "content"],
      "settings": {}
    }
  },
  "order": ["breadcrumb", "main"]
}
